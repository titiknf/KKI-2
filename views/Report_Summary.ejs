<html lang="en">
<head>

    <meta charset="UTF-8">
    <title><%= title %></title>
	 <link rel="Stylesheet" href="/stylesheets/bootstrap.min.css" />
    <link rel="Stylesheet" href="/stylesheets/template.css" />
    <link rel="Stylesheet" href="/stylesheets/menustyles.css" />
    <link rel="Stylesheet" href="/stylesheets/menu_sidebar.css" />
    <link rel="Stylesheet" href="/stylesheets/font-awesome/css/font-awesome.min.css" />
    <link rel="Stylesheet" type="text/css" media="all" href="/stylesheets/daterangepicker.css" />
 
    <script src="/javascripts/lib/jquery-2.1.4.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/javascripts/moment.js"></script>
    <script type="text/javascript" src="/javascripts/date.js"></script>

    <script type="text/javascript" src="/javascripts/daterangepicker.js"></script>
    <script src="/views/report_summary_views.js"></script>

    <script type="text/javascript">
       var host = '<%= host %>';
       var frm = new  clsReport_Summary_Views();
       frm.Fn_Initial();
    </script>

    <style type="text/css">
    	body{
    		background-color: #1a1a1a;
    	}

    	label.desc{
            color: white;
            text-align:right;
         }

        .myBox{
          font-size: 1em;
          padding: 6px;
          color: cyan;
          border: 2.5px solid #1B8EB9;
          font-weight: 300;
          box-sizing: border-box;
          height: 34px;
          text-indent: 6px;
          -webkit-box-shadow: inset 1px 0px 0px -1px #404040;
          box-shadow: inset 1px 0px 0px -1px #404040;
          transition: box-shadow 0.2s, border 0.2s, color 0.2s, background 0.2s;
          width: 150px;
           background: linear-gradient(
             #1a1a1a, #1a1a1a 50%, #333 50%, #333
          );
          background-size: 100% 202%;
          border-radius: 5px;
        }

        .myBox:focus{
          color:white;
          background-position: 100% 100%;
        }
    </style>
</head>
<body>
  <% include menu_sidebar.ejs %>
 
   <div class="container"  style="padding-top: 50px;">

   	   	<div class='row'>
   		<div class='col-md-12'>
		<div class="panel panel-default" style="background-color: #1a1a1a; border-color: #1a1a1a;">
			<div class="panel-heading" style="background-color: #1a1a1a; border-color: #1a1a1a;">
				<h3 style="color: cyan; margin-left: 430px;">รายงานแสดงรายวัน</h3>
				<hr>
			</div>
			<div class="panel-body">
				<div class="form-group row">
					<label for="txtStartDate" class="col-md-1 control-label text-rigth desc" style='text-align: right;margin-top: 5px'>วันที่ </label>
					<div class="form-group col-md-2">
					  <input type="text" class="form-control myBox" id="txtStartDate" >
					</div>
					<label for="txtEndDate" class="col-md-1 control-label text-rigth desc" style='text-align: right;margin-top: 5px'>ถึง วันที่ </label>
					<div class="form-group col-md-2">
					  <input type="text" class="form-control myBox" id="txtEndDate" >
					</div>
					<div class="col-md-offset-3 col-md-3" style='text-align: right; '>
						<input type="button"  id="btnSearch" style='margin-left: 10px' class="btn btn-info" value="Search">
						<input type="button"  id="btnExport" style='margin-left: 10px' class="btn btn-success" value="Export XLS">
					</div>
          		</div>
					

			 	<div class="row">
     				<div class="col-md-12">
				        <table class='table table-bordered table-strip table-condensed' id='tbResult'>
					       <thead class='bg-info'>
					          	<tr>
					          		<th colspan='4' >รายงานสรุป <span id='lbStart'></span> - <span id='lbEnd'></span></th>
					        	</tr>
					        	<tr>
					        		<th>Counter</th>
					        		<th>จำนวนคิวที่ให้บริการ</th> 
					        		<th>เวลารอ</th>
					        		<th>เวลาให้บริการ</th>
					        	</tr>
				 			</thead>
							 <tbody></tbody>
						</table>
					</div>  
			
				</div>
			</div>
		</div>
		</div>
		</div>
</body>
</html>